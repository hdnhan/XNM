//string path = "data/";
string path = "";
string filemesh = "Th1";
mesh Th = readmesh(path + filemesh + ".msh");
fespace Vh(Th,P1); Vh u,v;
Vh kappa=1+2*(x<-1)*(x>-2)*(y<3)*(y>-3);
solve a(u,v)= int2d(Th)(kappa*(dx(u)*dx(v)+dy(u)*dy(v)))+on(1,u=20);//+on(3, 4,u=60);
plot(u,wait=true, value=true, fill=true);